{
  "name": "smart-payment-buttons",
  "version": "2.0.7",
  "publishConfig": {
    "registry": "https://npm.paypal.com"
  },
  "description": "Serverside rendered ButtonJS",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.paypal.com/Checkout/smart-buttons"
  },
  "scripts": {
    "start": "node .",
    "setup": "npm install && npm run flow-typed",
    "lint": "eslint src/ server/ test/ *.js",
    "flow-typed": "flow-typed install --ignoreDeps peer && flow-typed install jest@22 mocha@3",
    "flow": "flow",
    "karma": "cross-env NODE_ENV=test babel-node --plugins=transform-es2015-modules-commonjs ./node_modules/.bin/karma start",
    "babel": "babel server/*.js server/**/*.js --exclude=node_modules --out-dir .",
    "webpack": "babel-node --plugins=transform-es2015-modules-commonjs ./node_modules/.bin/webpack --progress",
    "test": "npm run lint && npm run flow-typed && npm run flow && npm run karma && npm run jest",
    "build": "npm run test && npm run babel && npm run webpack",
    "release": "./publish.sh",
    "release:patch": "./publish.sh patch",
    "release:minor": "./publish.sh minor",
    "release:major": "./publish.sh major",
    "clean": "rimraf dist coverage",
    "reinstall": "rimraf flow-typed && rimraf node_modules && npm install && flow-typed install",
    "debug": "cross-env NODE_ENV=debug",
    "jest": "jest test/server --collectCoverage --env=node",
    "cover": "echo no-cover",
    "prepublish": "in-publish && npm run babel || not-in-publish",
    "postpublish": "git checkout config"
  },
  "peerDependencies": {
    "cal": "^0.5.44",
    "express": "^4.16.3",
    "servicecore": "^3",
    "xo-spark": "^5"
  },
  "dependencies": {
    "belter": "^1.0.19",
    "express": "^4.16.3",
    "grabthar": "^2.0.1",
    "module-babel": "^2.0.8",
    "request-promise": "^4.2.2",
    "vault": "^3.0.0",
    "zalgo-promise": "^1.0.28"
  },
  "devDependencies": {
    "flow-bin": "^0.70.0",
    "grumbler-scripts": "^2.0.56",
    "sync-browser-mocks": "^1.0.44"
  },
  "license": "PayPal",
  "author": "DL-PP-Hermes-Integration@paypal.com"
}
